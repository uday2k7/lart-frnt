@import url('https://fonts.googleapis.com/css?family=Lato:300,400,700&display=swap');

html{
	font-family: 'Lato', sans-serif;
}

ul{
	list-style:none;
	padding:0;
	margin:0;
}
a{
	text-decoration:none;
	color:#000000;
}

@media(min-width: 1400px){
.container{
	max-width:1280px;
}
}




/*///////header///////*/

.main_header{
	width:100%;
	padding:0;
	margin:0;
	clear:both;
	font-family: 'Lato', sans-serif;
	background:#ffffff;
}
.main_header .top_block{
	width:100%;
	display:inline-block;
	clear:both;
	padding:0;
	margin:25px 0;
	clear:both;
	text-align:center;
	position:relative;
}

.main_header .top_block .logo-block{
	display:inline-block;
	padding:0;
	margin:0;
	max-width:300px;
}

.main_header .top_block .logo-block img{
	width:100%;
}

.main_header .top_block .log-in_register_block{
	display: flex;
    padding: 0;
    margin: 0;
    font-family: 'Lato', sans-serif;
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    justify-content: center;
    align-items: center;
}

.main_header .top_block .log-in_register_block .menu_list{
	display: flex;
    padding: 0;
    margin: 0;
    clear: both;
    list-style: none;
    align-items: center;
}

.main_header .top_block .log-in_register_block .menu_list li{
	display:inline-block;
	padding:0 25px;
	margin:0;
	float:left;
	line-height:normal;
}
.main_header .top_block .log-in_register_block .menu_list a{
	display:inline-block;
	padding:10px;
	margin:0;
	cursor:pointer;
	text-decoration:none;
	font-family: 'Lato', sans-serif;
	font-weight:400;
	font-size: 15px;
	color:#000000;
	text-transform:uppercase;
}

/*/////////banner////////*/

.home-banner_block .carousel-indicators{ 
	margin-right: 2%;
    margin-left: 0;
    list-style: none;
    justify-content: flex-end;
}
.home-banner_block .carousel-indicators li{
	width: 16px;
    height: 16px;
	border-radius:100%;
}

.home-banner_block .carousel-caption h3{
	width:100%;
	padding:0;
	margin:0;
	font-family: 'Lato', sans-serif;
	font-weight:300;
	font-size: 42px;
	color:#ffffff;
	text-shadow: 1px 1px 11px rgba(150, 150, 150, 0.7);
}

.home-banner_block .carousel-caption p{
	width:100%;
	padding:0;
	margin:0;
	font-family: 'Lato', sans-serif;
	font-weight:300;
	font-size: 20px;
	color:#ffffff;
}


/*////already-log-in///*/
.main_header .top_block .log-in_register_block .notifications a{
	position: relative;
    color: #333333;
    padding: 0;
    margin: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}

.main_header .top_block .log-in_register_block .notifications .notifications-icon{
	content:"";
	display:inline-block;
	width:40px;
	height:40px;
	cursor:pointer;
	background:url(../images/notifications-icon.png) no-repeat;
	background-size: contain;	
}
.main_header .top_block .log-in_register_block .notifications .num{
	display: flex;
    width: 30px;
    height: 30px;
    padding: 0;
    background: #ffffff;
    border: 1px solid #999;
    font-family: 'Lato', sans-serif;
    font-size: 11px;
    color: #333333;
    text-align: center;
    position: absolute;
    top: -9px;
    right: 3px;
    border-radius: 100%;
    justify-content: center;
    align-items: center;
    line-height: normal;
}

/*////notifications/////*/
.main_header .top_block .log-in_register_block .notifications .dropdown-menu{
	position: relative;
	-webkit-box-shadow: -1px 1px 44px -9px rgba(0,0,0,0.38);
	-moz-box-shadow: -1px 1px 44px -9px rgba(0,0,0,0.38);
	box-shadow: -1px 1px 44px -9px rgba(0,0,0,0.38);
	top: 15px !important;
	padding: 0 !important;
}

.main_header.sticky .top_block .log-in_register_block .notifications .dropdown-menu{
	top: 10px !important;
}
::-webkit-scrollbar {
  width: 6px;
}

::-webkit-scrollbar-track {
  background: #f1f1f1; 
}
::-webkit-scrollbar-thumb {
  background: #888; 
}
::-webkit-scrollbar-thumb:hover {
  background: #555; 
}
.main_header .top_block .log-in_register_block .notifications .dropdown-menu .notifications-link{
	width: 100%;
	padding: 0;
	margin: 0;
	border: none;
	outline: none;
	max-height: 310px;
	overflow: auto;
}
.main_header .top_block .log-in_register_block .notifications .dropdown-menu::before{
	content: "";
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 0 8px 16px 8px;
	border-color: transparent transparent #ffffff transparent;
	position: absolute;
	top: -16px;
	right: 38px;
}
.notifications-button .dropdown-toggle{
	background: none !important;
    padding: 0;
    margin: 0;
    border: none !important;
    outline: none;
    text-align: center;
    width: auto;
    height: auto;
    display: flex;
    align-items: center;
    justify-content: center;
	flex-wrap: wrap;
	box-shadow: none !important;
}
.main_header .top_block .log-in_register_block .notifications .notifications-button .dropdown-item{
	position: relative;
    color: #333333;
    padding: 0;
    margin: 0;
    display: flex;
    justify-content: start;
    align-items: center;
    text-transform: none;
	background: #ffffff !important;
    padding: 10px;
    margin: 0;
    white-space: inherit;
    font-size: 13px;
	line-height: 15px;
	border-bottom: 1px solid #dedede;
}
.main_header .top_block .log-in_register_block .notifications .notifications-button .dropdown-item:hover{
	text-decoration: none;
	background: #f1f1f1 !important;
}
.main_header .top_block .log-in_register_block .notifications .notifications-button .dropdown-item.unread{
	background: #f1f1f1 !important;
	font-weight: bolder;
}
	
.notifications-button .dropdown-toggle .notifications-icon::after{
	display: none;
}
.main_header .top_block .log-in_register_block .register-button button.dropdown-toggle{
	display:inline-block;
	padding:0 50px 0 0;
	margin:0;
	clear:both;
	border:none !important;
	outline:none !important;
	text-decoration:none;
	background: url(../images/user-icon.png) no-repeat right top;
	background-size:40px;
	font-family: 'Lato', sans-serif;
	font-size: 14px;
	color:#333333;
	text-align:left;
	line-height:40px;
	position: relative;
    z-index: 99;
}
.register-button{
	position:relative;
}
.main_header .top_block .log-in_register_block .register-button button.dropdown-toggle::after{
	display:none;
}

.main_header .top_block .log-in_register_block .dropdown-menu{
	position: absolute;
    will-change: transform;
    top: 0px !important;
    right: -15px !important;
    transform: translate3d(0px, -20px, 0px);
    z-index: 9;
    width: 250px;
    left: inherit !important;
    padding: 15px 15px 15px 15px;
	margin:0;
}


/*//////*/
.main_header .main-menu_block{
	width:100%;
	padding:0;
	margin:0;
	border:none;
	outline:none;
	text-decoration:none;
	background:#ffffff !important;
	border-top:2px solid #6b6b6b;
	border-bottom:2px solid #6b6b6b;
}

.main_header .navbar{
	background:#ffffff !important;
	height:50px;
}

.main_header .navbar a{
	font-family: 'Lato', sans-serif !important;
	color: #000000 !important;
	font-size: 16px !important;
	text-transform:uppercase;
}

.main_header .navbar .navbar-nav{
	display: flex;
    width: 100%;
    flex-wrap: nowrap;
    flex-direction: inherit;
    justify-content: center;
}

.main_header .navbar .navbar-nav .nav-item a{
	font-family: 'Lato', sans-serif;
	font-size:16px;
	color:#000000;
}

.main_header .header_search-block{
	position: absolute;
    top: 6px;
    right: 0;
    background: #f2f2f2;
    border: none;
    outline: none;
	border-radius: 4px;
}

.main_header .header_search-block .form-control{
	width: 200px;
    background: #f2f2f2;
    border: none;
    outline: none;
    height: 38px;
    padding: 0 10px 0 45px;
    border-radius: 4px;
	font-family: 'Lato', sans-serif;
	font-size:15px;
	color:#666666;
	font-weight: 700;
	font-style:italic;
	border: none !important;
    outline: none !important;
	box-shadow: none;
}

.main_header .header_search-block .search-button{
	display: inline-block;
    width: 23px;
    height: 23px;
    padding: 0;
    margin: 0;
    border: none;
    outline: none;
    background: url(../images/search-icon.svg) no-repeat;
    background-size: 23px 23px;
    text-indent: -9999px;
    position: absolute;
    top: 50%;
    left:10px;
    cursor: pointer;
    margin-top: -12px;
}

.navbar-dark .navbar-toggler {
    padding:0;
	margin:0;
	border:none;
	outline:none;
}

.navbar-dark .navbar-toggler-icon{
	background-image:url(../images/menu.png) !important;
	background-size:contain;
	width:42px;
	height:42px;
	cursor:pointer;
	padding:0;
	margin:0;
}

@media(min-width: 1024px){
	.main_header .navbar ul.navbar-nav li{
		padding-left:30px;
		padding-right:30px;	
	}
}




/*/////////main_body////////*/

.main_body{
	width:100%;
	padding:30px 0 0 0;
	margin:0;
	clear:both;
}

.home-banner_block{
	width:100%;
	padding:0;
	margin:0;
	border:none;
	outline:none;
	text-align:center;
	background:#000000;
}
.home-banner_block .carousel-control-next, .home-banner_block .carousel-control-prev{
	opacity: 0;
}



.main_body .home-text_block{
	width:100%;
	padding:60px 0;
	margin:0;
	clear:both;
	border:none;
	outline:none;
	text-align:center;
}



.main_body .home-text_block .text-box{
	width: 100%;
    display: flex;
    max-width: 100%;
    padding: 0;
    margin: 0;
    clear: both;
    border: none;
    outline: none;
    text-align: center;
    flex-wrap: wrap;
    justify-content: center;
}
.main_body .home-text_block .add-border{
	border-left:1px solid #333333;
	border-right:1px solid #333333;
}


.main_body .home-text_block .text-box .text-box_header{
	width:100%;
	height:128px;
	padding:0;
	margin:0;
	clear:both;
	border:none;
	outline:none;
	text-decoration:none;
	font-family: 'Lato', sans-serif;
	font-weight: 400;
	font-style: normal;
	color: #ffffff;
    font-size: 24px;
	min-height: 60px;
	text-align:center;
	background-size: contain !important;
    display: flex;
    align-items: center;
    justify-content: center;
}
.main_body .home-text_block .text-box .text-box_header span{
	max-width:340px;
	padding:0;
	margin:0;
}

.main_body .home-text_block .emerging-artists .text-box .text-box_header{
	background:url(../images/brushstroke-1.jpg) no-repeat center;
}
.main_body .home-text_block .emerging-artists .text-box .text-box_header span{
	max-width:240px;
}
.main_body .home-text_block .your-artwork .text-box .text-box_header{
	background:url(../images/brushstroke-2.jpg) no-repeat center;
}
.main_body .home-text_block .your-artwork .text-box .text-box_header span{
	max-width:200px;
}
.main_body .home-text_block .about-luddart .text-box .text-box_header{
	background:url(../images/brushstroke-3.jpg) no-repeat center;
}

.main_body .home-text_block .text-box p{
	width:100%;
	max-width:300px;
	padding:0;
	margin:25px 0;
	clear:both;
	border:none;
	outline:none;
	text-decoration:none;
	font-family: 'Lato', sans-serif;
	font-weight: 400;
	font-style: normal;
	color: #333333;
    font-size: 16px;
	line-height:19px;
	min-height: 85px;
	text-align: center;
}
.main_body .home-text_block .text-box .text-box_more{
	display: flex;
    justify-content: center;
	width:100%;
	padding:0;
	margin:0;
	clear:both;
	border:none;
	outline:none;
	text-align:center;
}
.main_body .home-text_block .text-box .text-box_more .more-button{
	display: flex;
    justify-content: center;
    align-items: center;
	width: 178px;
	height: 40px;
	background-color: #ffffff;
	outline:none;
	text-decoration:none;
	cursor:pointer;
	font-family: 'Lato', sans-serif;
	font-weight: 400;
	font-style: normal;
	color: #000000;
    font-size: 15px;
	text-transform:uppercase;
	padding:0;
	margin:0;
	border: 1px solid #707070;
	border-radius:6px;
	
	-webkit-box-shadow: 0px 3px 6px 0px rgba(0,0,0,0.1);
	-moz-box-shadow: 0px 3px 6px 0px rgba(0,0,0,0.1);
	box-shadow: 0px 3px 6px 0px rgba(0,0,0,0.1);
}



.main_body .image-slider_block{
	width:100%;
	display:inline-block;
	padding:40px 0;
	margin:25px 0 0 0;
	clear:both;
	border:none;
	outline:none;
	border-top:1px solid #cacaca;
	position:relative;
}

.main_body .image-slider_block:after{
	content:"";
	width:50px;
	height:12px;
	position:absolute;
	top:-6px;
	right:0;
	background:#ffffff;
}

.main_body .image-slider_block:before{
	content:"";
	width:50px;
	height:12px;
	position:absolute;
	top:-6px;
	left:0;
	background:#ffffff;
}

.main_body .image-slider_block .slider-image{
	padding:0;
	margin:0;
	position:relative;
}




.carousel-control-next, .carousel-control-prev{
	z-index:9999;
}

.main_body .image-slider_block .carousel-caption{
	background:none;
	padding:20px 5px;
	font-family: 'Lato', sans-serif;
    font-weight: 400;
    font-style: normal;
    font-size: 24px;
    color: #000000;
    text-align: center;
	position:static !important;
}

.main_body .image-slider_block .carousel-caption h3{
	width:100%;
	padding:0;
	margin:0;
	border:none;
	outline:none;
	font-family: 'Lato', sans-serif;
    font-weight: 400;
    font-style: normal;
    font-size: 24px;
	line-height:29px;
    color: #000000;
    text-align: center;
	text-shadow:none;
}


.main_body .image-slider_block.luddspace_block .carousel-caption p{
	width:100%;
	padding:0;
	margin:0;
	border:none;
	outline:none;
	font-family: 'Lato', sans-serif;
    font-weight: 400;
    font-style: normal;
    font-size: 24px;
	line-height:29px;
    color: #000000;
    text-align: center;
	text-shadow:none;
}

.main_body .image-slider_block .page_header{
	width:100%;
	padding:0 0 25px 0;
	margin:0;
	clear:both;
	border:none;
	outline:none;
	position:relative;
	text-align:center;
}

.main_body .image-slider_block .page_header .header-text{
	width:100%;
	padding:0;
	margin:0;
	clear:both;
	border:none;
	outline:none;
	position:relative;
	text-align:center;
	font-family: 'Lato', sans-serif;
    font-weight: 400;
    font-style: normal;
    font-size: 48px;
    text-align: center;
	color: #000000;
	text-transform:uppercase;
}

.main_body .image-slider_block .page_header .see-all{
	display:inline-block;
	position:absolute;
	top:0;
	right:60px;
	padding:0;
	margin:0;
}
.main_body .image-slider_block .page_header .see-all .see-all_link{
	display:inline-block;
	padding:0 10px;
	margin:0;
	border:none;
	outline:none;
	text-decoration:none;
	cursor:pointer;
	font-family: 'Lato', sans-serif;
    font-weight: 400;
    font-style: normal;
	font-size: 15px;
	line-height:48px;
	color: #000000;
	text-transform:uppercase;
}

.carousel-control-prev-icon{
	background-image:url(../images/slider_left_arrow.svg);
}
.carousel-control-next-icon{
	background-image:url(../images/slider_right_arrow.svg);
}

.carousel-control-next-icon, .carousel-control-prev-icon{
	width: 20px;
    height: 32px;
}


/*//////////login/////////*/

.login-register_main-block{
	width:100%;
	max-width:650px;
	padding:20px;
	margin:0 auto;
	clear:both;
	display: flex;
	align-items: start;
	justify-content: center;
	border:1px solid #797979;
	text-align:center;
